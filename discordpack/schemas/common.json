        "EventTrigger": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/EventTrigger",
            "type": "object",
            "default": {},
            "properties": {
                "conditions": {
                    "$ref": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/predicate.json"
                },
                "event": {
                    "description": "The event to run.",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "Channel": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Channel",
            "description": "The channel id",
            "anyOf": [
                {
                    "description": "The command variable to use get the channel id from",
                    "type": "string",
                    "examples": [
                        "this",
                        "system",
                        "member"
                    ]
                },
                {
                    "description": "The channel id",
                    "type": "integer"
                }
            ],
            "default": "this"
        },
        "Message": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Channel",
            "anyOf": [
                {
                    "description": "The command variable to use get the message id from",
                    "type": "string",
                    "examples": [
                        "this"
                    ]
                },
                {
                    "description": "The message id",
                    "type": "integer"
                }
            ],
            "default": "this"
        },
        "Emoji": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Emoji",
            "description": "Can be any of; custom emoji name, custom emoji id, command variable, pasted emoji (i.e. '☻')",
            "type": [
                "string",
                "integer",
                "object"
            ],
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "description": "The name of the emoji. :smile:",
                    "type": "string",
                    "examples": [
                        ":smile:"
                    ]
                }
            },
            "additionalProperties": false
        },
        "Guild": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Guild",
            "anyOf": [
                {
                    "description": "The command variable to use get the guild id from",
                    "type": "string",
                    "examples": [
                        "this"
                    ]
                },
                {
                    "description": "The guild id",
                    "type": "integer"
                }
            ],
            "default": "this"
        },
        "Member": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Member",
            "anyOf": [
                {
                    "description": "The command variable to use get the member from",
                    "type": "string",
                    "examples": [
                        "this"
                    ]
                },
                {
                    "description": "The member id",
                    "type": "integer"
                }
            ],
            "default": "this"
        },
        "Conditions": {
            "$id": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/common.json#/definitions/Conditions",
            "anyOf": [
                {
                    "description": "resrouce location to a predicate",
                    "type": "string"
                },
                {
                    "type": [
                        "object",
                        "array"
                    ],
                    "$ref": "https://raw.githubusercontent.com/legopitstop/Python_Projects/main/discordpack/schemas/predicate.json"
                }
            ]
        }
